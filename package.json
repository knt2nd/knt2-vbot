{
  "name": "knt2-vbot",
  "version": "1.1.10",
  "description": "Kanata 2nd Voice Bot is a smart speaker over the Internet, supports lots of languages.",
  "author": "Kanata",
  "license": "MIT",
  "private": true,
  "scripts": {
    "init": "npm i && node tasks init",
    "build": "run-s prebuild build-prd",
    "start": "run-p start-prd start-plugins",
    "dev": "run-p start-dev start-plugins",
    "prebuild": "rimraf ./dist && copyfiles -u 2 \"./src/assets/**/*\" ./dist && copyfiles -u 2 \"./src/plugins/!(!*)/assets/**/*\" ./dist/plugins",
    "start-plugins": "node tasks start-plugins",
    "start-prd": "http-server ./dist/ -a localhost -c-1 -o",
    "start-dev": "run-s prebuild server-dev",
    "build-prd": "webpack --mode production",
    "server-dev": "webpack-dev-server",
    "lint": "eslint --ext js,vue ./src/app/ ./src/plugins/ ./plugin-example/",
    "beatify": "eslint --fix --ext js,vue ./src/app/ ./src/plugins/ ./plugin-example/",
    "firebase": "firebase"
  },
  "dependencies": {
    "@mdi/font": "^5.3.45",
    "axios": "^0.21.1",
    "axios-jsonp": "^1.0.4",
    "copyfiles": "^2.3.0",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "he": "^1.2.0",
    "howler": "^2.2.0",
    "html-webpack-plugin": "^4.3.0",
    "http-server": "^0.12.3",
    "js-queue": "^2.0.0",
    "js-yaml": "^3.14.0",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.3.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-plugin-vue": "^6.2.2",
    "firebase-tools": "^9.6.0"
  }
}
